ALGORITHM :diffrenent integer
VAR
set1:ARRAY _OF INTEGER [l];
set2::ARRAY _OF INTEGER [m];
i,j:INTEGER;
s:INTEGER;
count:INTEGER;
htab : HASH_TABLE<INTEGER,INTEGER>;
BEGIN
Write ("what is your set 1 length : ")
          read (L);
write ("what is your set 2 length : ")
          read (m);
s:=0
FOR i FROM 0 TO set1.length -1 STEP 1  DO
htab.insert(set1[i],1);
FOR j FROM 0 TO set2.length -1 STEP 1  DO
 
  if htab.lookup(set2[j])=false) then
   count := htab.get(set1[i],1);
   htab.insert(set2[j],1)
  s:= s+set1[i]+set2[j]
  ELSE
  htab..insert(set2[j],count+1)
  END -IF
  END-FOR;
  END-FOR;
   Write(htab);
 write(" somme is" s) ;
   END 
   

