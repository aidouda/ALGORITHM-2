BEGIN
Write ("what is your array 1 length : ")
          read (L);
write ("what is your array  2 length : ")
          read (m);

FOR i  FROM 0 to array 1 length -1 STEP 1 DO
FOR j FROM 0 to  array 2 length-1 STEP 1 DO
k:=0
IF array 1[i]<>array 2[j]
array 3[k]=array 1[i]
Write (array 3[k])
k:=k+1;
